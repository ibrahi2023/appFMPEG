<!DOCTYPE html>
<html lang="en" >
<head>
  <meta charset="UTF-8">
  <title>SCALEAUTO</title>
  <link rel="stylesheet" href="/stylesheets/style.css">
  <!-- <link href="/stylesheets/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
  -->
</head>
<body class="flex bg-gray-100 min-h-screen" >
  <aside class="hidden sm:flex sm:flex-col">
    <a href="#" class="inline-flex items-center justify-center h-20 w-20 bg-purple-600 hover:bg-purple-500 focus:bg-purple-500">
      <span class="h-18 w-18  sm:ml-1 mr-2 bg-gray-100 rounded-full overflow-hidden">
        <img src="/images/ffmpeg.png" alt="user profile photo" class="h-full w-full object-cover">
    </a>
    <div class="flex-grow flex flex-col justify-between text-gray-500 bg-gray-800">
      <div class="flex flex-col center justify-center h-2 w-20 border-t border-gray-700 pt-10">
        <h5 style="margin-top: 270px;" class="ml-2">Scale@</h5>
        <form action="/scaleAuto" method="get"   class="ml-1 mt-0  flex-col bg">
          <input type="text" name="user" value="<%= user %>" hidden>
          <button   style="width: 70px;" type="submit" class="py-4 bg-orange-700 border border-gray-700  shadow-xl rounded-md px-1 hover:bg-gray-500 focus:bg-orange-500 ">
              <span class="font-thin text-orange-100">Auto</span> 
             </button>
          </form>
        
      <form action="scal256@144" method="get"   class="ml-1 mt-2  flex-col bg">
        <input type="text" name="user" value="<%= user %>" hidden>
        <button   style="width: 70px;" type="submit" class="py-4 bg-gray-700 border border-gray-700  shadow-xl rounded-md px-1 hover:bg-gray-500 focus:bg-orange-500 ">
          <span class="font-thin text-orange-100">256:144</span> 
         </button>
      </form>
        <form action="scal720@480" method="get"  class="ml-1 mt-2 flex-col bg ">
          <button  style="width: 70px;" type="submit" class="py-4 bg-gray-700 border border-gray-700 shadow-xl rounded-md px-1 hover:bg-gray-500 focus:bg-orange-500">
          <span class="font-thin text-orange-100">720:480</span> 
       </button>
      </form>
        <form action="scal1190@1080" method="get"  class="ml-1 mt-2 flex-col bg ">
          <input type="text" name="user" value="<%= user %>" hidden>
          <button  style="width: 70px;" type="submit" class="py-4 bg-gray-700 border border-gray-700 shadow-2xl rounded-md px-1 hover:bg-gray-500 focus:bg-orange-500">
          <span class="font-thin text-orange-100">1190:1080</span> 
          </button>
       </form>
       <div class="ml-1 mt-2  flex-col bg">
        <div style="width: 70px;"  class="py-4 bg-orange-300 border border-orange-300  shadow-xl rounded-md px-1 hover:bg-gray-300 focus:bg-orange-300 "     class="font-extralight text-orange-300 py-0 px-2 mb-1 border border-orange-300  hover:bg-orange-100 rounded-md flex h-6 w-28">
         <a href=""  id="dowloadurl" onclick="downloadlink();"><img  class="ml-4"   src="data:image/svg+xml;base64," width="28" height="28"></a>
         </div>
      </div>
     </div>
    </div>
  </aside>
  <div class="flex-grow text-gray-800">
    <header class="flex items-center h-20 px-6 sm:px-10 bg-white">
      <h3 id="message" ondblclick='ResetMSG()'></h3>
      <button class="block sm:hidden relative flex-shrink-0 p-2 mr-2 text-gray-600 hover:bg-gray-100 hover:text-gray-800 focus:bg-gray-100 focus:text-gray-800 rounded-full">
        <span class="sr-only">Menu</span>
          <svg aria-hidden="true" fill="none" viewBox="0 0 24 24" stroke="currentColor" class="h-6 w-6">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 6h16M4 12h16M4 18h7" />
        </svg>
      </button>
      <div class="flex flex-shrink-0 items-center ml-auto">
        <button class="inline-flex items-center p-2 hover:bg-gray-100 focus:bg-gray-100 rounded-lg">
          <span class="sr-only">User Menu</span>
          <div class="hidden md:flex md:flex-col md:items-end md:leading-tight">
            <span class="font-semibold">IPCameraFFmpeg</span>
            <span class="font-thin"><%= user %></span>
           </div>
          <span class="h-12 w-12 ml-2 sm:ml-3 mr-2 bg-gray-100 rounded-full overflow-hidden">
            <img src="/images/insc.png" alt="user profile photo" class="h-full w-full object-cover">
          </span>
          <svg aria-hidden="true" viewBox="0 0 20 20" fill="currentColor" class="hidden sm:block h-6 w-6 text-gray-300">
            <path fill-rule="evenodd" d="M5.293 7.293a1 1 0 011.414 0L10 10.586l3.293-3.293a1 1 0 111.414 1.414l-4 4a1 1 0 01-1.414 0l-4-4a1 1 0 010-1.414z" clip-rule="evenodd" />
          </svg> 
        </button>
        <div class="flex border-l pl-3 ml-3 space-x-1">
          <button class="flex-1 relative p-2 text-gray-400 hover:bg-gray-100 hover:text-gray-600 focus:bg-gray-100 focus:text-gray-600 rounded-full">
            <span class="sr-only">Notifications</span>
            <span class="absolute top-0 right-0 h-2 w-2 mt-1 mr-2 bg-red-500 rounded-full"></span>
            <span class="absolute top-0 right-0 h-2 w-2 mt-1 mr-2 bg-red-500 rounded-full animate-ping"></span>
            <svg aria-hidden="true" fill="none" viewBox="0 0 24 24" stroke="currentColor" class="h-6 w-6">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 17h5l-1.405-1.405A2.032 2.032 0 0118 14.158V11a6.002 6.002 0 00-4-5.659V5a2 2 0 10-4 0v.341C7.67 6.165 6 8.388 6 11v3.159c0 .538-.214 1.055-.595 1.436L4 17h5m6 0v1a3 3 0 11-6 0v-1m6 0H9" />
            </svg>
          </button>
          <form action="/LivestreaMPD" method="get"  class="flex-1"> <input type="text" name="user" value="<%= user %>" hidden>
            <button class="relative p-2 text-gray-400 hover:bg-gray-100 hover:text-gray-600 focus:bg-gray-100 focus:text-gray-600 rounded-full">
            <span class="sr-only">Log out</span>
            <svg aria-hidden="true" fill="none" viewBox="0 0 24 24" stroke="currentColor" class="h-6 w-6">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 16l4-4m0 0l-4-4m4 4H7m6 4v1a3 3 0 01-3 3H6a3 3 0 01-3-3V7a3 3 0 013-3h4a3 3 0 013 3v1" />
           </svg>
          </button></form> 
        </div>
      </div>
    </header>
    <main class="flex-1 items-left  ml-40 mt-4 px-1 w-full h-fit">
        <h1 class="text-gray-500 text-balance mb-2">Live tream MPD Dash:ABR Roles</h1>
        <div class="h-full max-h-80">
         <div class="grid grid-rows-3 grid-flow-col gap-1 w-4/6">
            <div id="scale256" style="height: 80vh;" class="row-start-1 row-span-4 bg-gray-200 shadow-md rounded-md h-90 px-1 py-2">
            <!-- <video  data-dashjs-player="" autoplay src="https://dash.akamaized.net/akamai/bbb_30fps/bbb_30fps.mpd" controls="true" style="width:100%;height:100%;border-radius: 8px 8px;"></video> -->
            <video id="video" 
            width='480px' height='360px' controls autoplay
            src="/output/HEVC/playList.m3u8" style="width:100%;height:100%;border-radius: 8px 8px;">
     </video>
            </div>
            </div>
    </main>
    </div>
   </body>
    <script src="/javascripts/hls.min.js"></script>
 <!-- <script type="text/javascript" src="/javascripts/jsmpeg.min.js"></script> -->
  <!-- <script src="/javascripts/dash.all.min.js"></script> -->
<script>

  document.getElementById("message").innerHTML="";
            if (Hls.isSupported()) {
              var video = document.getElementById('video');
              var hls = new Hls();
              hls.loadSource(video.src);
              hls.attachMedia(video);
            }else{
                alert("Cannot stream HLS, use another video source");
            }
    function downloadlink(){
    const link="/downloadVideo";
    const link2=" https://raw.githubusercontent.com/ibrahi2023/appFMPEG/main/public/output/autoscale/Dolphin.mpd";
  document.getElementById(`dowloadurl`).href=link;
  document.getElementById("message").innerHTML=`<span class="text-orange-500">Copier URL Video:</span><span >${link2}</span>`
 
    }
 function ResetMSG(){
  document.getElementById("message").innerHTML="";
 
 }
    </script>
</html>


